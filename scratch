Eratosthenes(n) {
  a[1] := 0                          
  for i := 2 to n do a[i] := 1
  p := 2
  while p^2  <  n do {
    j := p^2
    while (j  <  n) do {             
      a[j] := 0
      j := j+p
    }
    repeat p := p+1 until a[p] = 1   
  }
  return(a)
}

Eratosthenes(10)
// 2,3,5,7

array[1] = 0
for i := 2 to 10 do a[2] := 1
p := 2
while 2^2 < 10 do
  j := 2^2
  while (2^2 < 10) do
    a[j] := 0
    j := 2^2 + 2
  repeat p := p+1 until a[p] = 1
  

